{
  "HttpConfig": {
    "SupportedVersions": [
      "http/1.1", // list of support http versions - use '*' to support all versions
      "http/1.2",
      "http/2",
      "http/2.0",
      "http/2.1"
    ],
    "RequireSSL": false
  },
  "CrossOriginConfig": {
    "AllwedOrigins": [
      "*" // use * to allow all origins - default is '' (or none)
    ],
    "ExposeHeaders": [],
    "AllowCredentials": false // default value is false
  },
  "HeaderValidationConfig": {
    "MaxHeaderLength":  10000
  }
}